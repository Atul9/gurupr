{
  "name": "gurupr",
  "scripts": {
  },
  "env": {
    "CLOUDINARY_URL": {
      "required": true
    },
    "DATABASE_URL": {
      "required": true
    },
    "DEFAULT_URL": {
      "required": true
    },
    "GITHUB_KEY": {
      "required": true
    },
    "GITHUB_SECRET": {
      "required": true
    },
    "LANG": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "required": true
    }
  },
  "formation": {
    "worker": {
      "quantity": 1
    },
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "heroku-postgresql",
    "rollbar"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ]
}
